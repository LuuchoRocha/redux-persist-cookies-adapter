{
  "compilerOptions": {
    "target": "es5",
    "lib": ["esnext", "dom"],
    "module": "commonjs",
    "rootDir": "./",
    "moduleResolution": "node",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": false,
    "declaration": true,
    "emitDeclarationOnly": true
  }
}
